<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.2</TargetFramework>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <Version>0.0.5</Version>
    <Company>LetPortal</Company>
    <Product>LetPortal</Product>
    <Authors>An Le Quang Phuc</Authors>
    <Description>LetPortal is an internal web platform which helps small company to manage their data. The project is open-source project. There are no fee charged to use or modify.</Description>
    <Copyright>An Le Quang Phuc</Copyright>
    <PackageProjectUrl>http://letportal.app</PackageProjectUrl>
    <PackageTags>Web Portal; .NET Core;</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Logger\Attributes\**" />
    <EmbeddedResource Remove="Logger\Attributes\**" />
    <None Remove="Logger\Attributes\**" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Diagnostics.Tracing.TraceEvent" Version="2.0.42" />
    <PackageReference Include="Microsoft.Extensions.Options" Version="2.2.0" />
    <PackageReference Include="MongoDB.Driver" Version="2.9.1" />
    <PackageReference Include="MySql.Data" Version="8.0.19" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.2" />
	<PackageReference Include="Microsoft.AspNetCore.App" />
	<PackageReference Include="Npgsql" Version="4.0.10" />
	<PackageReference Include="Polly" Version="7.1.0" />
	<PackageReference Include="Serilog" Version="2.8.0" />
	<PackageReference Include="Serilog.Settings.Configuration" Version="3.1.0" />
	<PackageReference Include="Serilog.Sinks.Console" Version="3.1.1" />
	<PackageReference Include="Serilog.Sinks.File" Version="4.0.0" />
	<PackageReference Include="Serilog.Sinks.MongoDB" Version="4.0.0" />
	<PackageReference Include="Serilog.Sinks.RollingFile" Version="3.3.0" />
	<PackageReference Include="TimeZoneConverter" Version="3.2.0" />
  </ItemGroup>
</Project>
